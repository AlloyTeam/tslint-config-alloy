{
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ]
    },
    "meta": {
        "category": "maintainability",
        "description": "限制对象、数组、解构赋值等的最后一项末尾是否需要逗号",
        "has-fixer": true,
        "prettier": true
    }
}
